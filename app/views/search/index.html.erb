
<% @stations.each do |station| %>
<div id="station-<%= station["id"]%>">
  <ul>
    <li><%= station["station_name"] %> </li>
    <li><%= station["street_address"]%>, <%= station["city"] %> <%= station["state"] %> <%= station["zip"] %> </li>
    <li><%= station["fuel_type_code"] %> </li>
    <li>Distance: <%= station["distance"] %> miles</li>
  </ul>
</div>
<% end %>
